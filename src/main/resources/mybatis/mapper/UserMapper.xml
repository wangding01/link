<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cn.linka.business.mapper.UserMapper">

    <select id="queryUserList" resultType="com.cn.linka.business.dao.User">
        select * from user
    </select>
    <select id="selectByEmail" resultType="java.lang.String">
        select * from user where email = #{email}
    </select>
    <insert id="insert" parameterType="com.cn.linka.business.dao.User">
        INSERT INTO `user` (
            `user_id`,
            `user_name`,
            `password`,
            `phone`,
            `email`,
            `create_dt`
        )
        VALUES
            (
                #{userId},
                #{userName},
                 #{password},
                #{phone},
                #{email},
                #{createDt}
            )
    </insert>

</mapper>
